{
    "manifestVersion": 1,
    "id": "Twilio-Send-SMS",
    "name": "Twilio Send SMS",
    "version": "G_VERSION",
    "publisher": "DanielMeixner",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Daniel Meixner's build and release tools.",
    "categories": [
        "Build and release"
    ],
    "icons": {
        "small": "/images/icon64.png",
        "default": "/images/icon.png"
    },
    "content": {

        "details": {

            "path": "overview.md"
        }
    },
    "files": [
        {
            "path": "buildtask"
        }
        
    ],
    "contributions": [
        {
            "id": "custom-build-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildtask"
            }
        }
    ]
}